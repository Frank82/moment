      </div>
    </div>

    <div class="middle">
      <div class="photos">
        <img src="assets/img1.jpg" />
      </div>
      <div class="photos">
        <img src="assets/img3.jpg" />
      </div>
      <div class="photos">
        <img src="assets/img5.jpg" />
      </div>
      <div class="photos">
        <img src="assets/img6.jpg" />
      </div>
    </div>

    <div class="user-right">
      <ul>
        <li><a href="\">Home</a></li>
        <li><a href="">Photos</a></li>
        <%= link_to "Post", new_post_path %>
      </ul>
    </div>
  </div>
